# Copy to .env and fill in values

# OAuth client credentials (for `--auth` loopback flow)
GOOGLE_OAUTH_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your_client_secret

# Target Drive folder and local directory
DRIVE_FOLDER_ID=
LOCAL_DIR=./docs

# Optional: select a profile name for token separation
# PROFILE=work
